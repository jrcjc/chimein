noinst_HEADERS = config.h package_revision.h
PO_DIR = po
DESKTOP_FILE = pidgin.desktop
#appsdir = $(datadir)/applications
#apps_in_files = pidgin.desktop.in
#apps_DATA = $(apps_in_files:.desktop.in=.desktop)
